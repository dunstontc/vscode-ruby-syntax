        {
          "begin": "\\{",
          "end": "\\}",
          "patterns": [
            {
              "include": "#braces"
            }
          ]
        }
      ]
    },
    "brackets": {
      "patterns": [
        {
          "match": "\\\\\\[|\\\\\\]|\\\\\\\\",
          "name": "constant.character.escape.ruby"
        },
        {
          "begin": "\\[",
          "end": "\\]",
          "patterns": [
            {
              "include": "#brackets"
            }
          ]
        }
      ]
    },
    "parens": {
      "patterns": [
