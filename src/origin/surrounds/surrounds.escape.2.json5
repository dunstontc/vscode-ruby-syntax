{
  "repository": {
    "angles": {
      "patterns": [
        {
          "match": "\\\\<|\\\\>|\\\\\\\\",
          "name": "constant.character.escape.ruby"
        },
        {
          "begin": "<",
          "end": ">",
          "patterns": [
            {
              "include": "#angles"
            }
          ]
        }
      ]
